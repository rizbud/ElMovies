{
  "compilerOptions": {
    "target": "es2017",
    "module": "es6",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./App",
    "paths": {
      "@Components/*": ["Components/*"],
      "@Components": ["Components"],
      "@Config/*": ["Config/*"],
      "@Containers/*": ["Containers/*"],
      "@Images/*": ["Images/*"],
      "@Images": ["Images"],
      "@Lib/*": ["Lib/*"],
      "@Navigation/*": ["Navigation/*"],
      "@Reduxes/*": ["Reduxes/*"],
      "@Sagas/*": ["Sagas/*"],
      "@Services/*": ["Services/*"],
      "@Themes/*": ["Themes/*"],
    }
  },
  "include": ["App"],
  "exclude": ["node_modules", "dist", "android", "ios", "Tests"]
}